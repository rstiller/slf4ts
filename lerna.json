{
  "command": {
    "publish": {
      "ignoreChanges": [
        "ignored-file",
        "*.md"
      ]
    },
    "bootstrap": {
      "npmClientArgs": [
        "--no-package-lock"
      ]
    }
  },
  "npmClient": "npm",
  "stream": true,
  "parallel": false,
  "packages": [
    "packages/*"
  ],
  "version": "independent"
}
